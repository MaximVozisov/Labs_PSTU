#include <iostream>

using namespace std;

//Лабораторная работа N5

int main()
{
	setlocale(0, "");
	srand(time(0));
	int m, n;
	cout << "Столбцов: ";
	cin >> m;
	cout << "Строк: ";
	cin >> n;
	int** a = new int *[n];

	for (int i = 0; i < n; i++)
	{
		a[i] = new int[m];
	}

	for (int i = 0; i < n; i++) 
	{
		for (int j = 0; i < m; j++)
		{
			a[i][j] = rand() % 100;
			cout << a[i][j] << " ";
		}
		cout << endl;
	}
	delete[] a;
	return 0;
}
